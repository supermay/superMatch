<h3>Your Profile</h3>
<div class="table-responsive">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Address</th>
        <th>Image</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= @profile.first_name %></td>
        <td><%= @profile.last_name %></td>
        <td><%= @profile.address %></td>
        <th><%= image_tag @profile.image.thumbnail %></th>
      </tr>
    </tbody>
  </table>
</div>
